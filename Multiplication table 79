import java.util.*;
public class Solution {

    public static void main(String[] args) {
        Scanner s=new Scanner(System.in);
        int num=s.nextInt();
        if(num>=1 && num<=9){
            for(int i=1;i<=num;i++){
                System.out.printf("%d x %d = %d\n",num,i,num*i);
                
            }
        }
        else{
            System.out.println("Invalid Input");
        }
    }
}
