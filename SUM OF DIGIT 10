import java.util.*;
public class Solution {

    public static void main(String[] args) {
        Scanner s=new Scanner(System.in);
        int n=s.nextInt();
        if(n>100){
            int sum=0;
            while(n>0){
                sum=sum+n%10;
                n=n/10;
            }
            System.out.println("Sum of digit is "+sum);
        }
        else{
             System.out.println("Invalid Input");
        }
    }
}
